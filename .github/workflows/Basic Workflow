name:CI

on:push or pull

push:
    branches: [ "main" ]
  pull_request:
    branches: [ "main" ]
    
     workflow_dispatch:
     
     jobs:
     build:
     runs-on:
     
     
     steps:
     
      - uses: actions/checkout@v4
      
     - name: Run a one-line script
     
     run: echo Hello, world!